{
  "name": "content-manager",
  "version": "1.0.0",
  "description": "Back-end of content manaager",
  "main": "./server/index.js",
  "scripts": {
    "test": "mocha",
    "deploy": "npm install --production && npm run pm2:prd",
    "pm2:stop": "./node_modules/pm2/bin/pm2 kill",
    "pm2:list": "./node_modules/pm2/bin/pm2 list",
    "pm2:dev": "./node_modules/pm2/bin/pm2 startOrReload ecosystem.config.js --env development",
    "pm2:prd": "./node_modules/pm2/bin/pm2 startOrReload ecosystem.config.js --env production",
    "start": "node ./server/index.js",
    "server:prd": "set NODE_ENV=production && node ./server/index.js --env production",
    "watch": "webpack --watch",
    "build": "webpack --env.production",
    "ipt": "nodemon --inspect-brk ./server/index.js --env.development -w ./server"
  },
  "repository": {
    "type": "git",
